<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 以后新增的任何系统组件(View)名都用简写（取单词首字母，SurfaceView简写为sfv）；其它都不简写 -->


    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="wrap_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- page <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="activity_page">
        <item name="android:splitMotionEvents">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/activity_bg</item>
        <item name="android:orientation">vertical</item>
        <!-- 状态栏沉浸 <<<<<<<<<<<<<<<<< -->
        <item name="android:clipToPadding">true</item>
        <item name="android:fitsSystemWindows">true</item>
        <!-- 状态栏沉浸 >>>>>>>>>>>>>>>>> -->
    </style>

    <style name="bottom_window_page" parent="activity_page">
        <item name="android:gravity">bottom|center_horizontal</item>
        <item name="android:background">@color/alpha_complete</item>
    </style>

    <style name="bottom_window_content" parent="ll_vertical_match_wrap">
        <item name="android:background">@color/activity_bg</item>
        <item name="android:padding">@dimen/padding_huge</item>
    </style>

    <style name="bottom_window_icon" parent="text_middle">
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">45sp</item>
        <item name="android:background">@drawable/null_drawable</item>
    </style>

    <style name="bottom_window_button" parent="text_middle_black">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/bg_white_to_alpha</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="bottom_window_button_cancel" parent="bottom_window_button">
        <item name="android:text">取消</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_gray_to_alpha</item>
    </style>

    <!-- page >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


